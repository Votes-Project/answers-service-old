{
  "name": "answers-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "bun test test/",
    "start": "fastify start -l info src/app.mjs",
    "dev": "yarn build && concurrently -k -p \"[{name}]\" -n \"Rescript,App\" -c \"yellow.bold,cyan.bold\" \"yarn watch:res\" \"yarn dev:start\"",
    "dev:start": "fastify start -w -l info -P src/app.mjs",
    "build": "npm run build:edgedb && npm run build:res",
    "build:res": "rescript build",
    "watch:res": "rescript build -w",
    "build:edgedb": "rescript-edgedb generate --output ./db/__generated__ --src ./db",
    "watch:edgedb": "npm run build:edgedb -- --watch"
  },
