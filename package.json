    "@fastify/sensible": "^5.0.0",
    "@rescript/core": "0.5.0",
    "concurrently": "^8.2.2",
    "edgedb": "^1.3.7",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.3.0",
    "fastify-plugin": "^4.0.0",
    "postgraphile": "^4.13.0",
    "rescript": "11.0.0-rc.4",
    "rescript-edgedb": "^0.3.0",
    "rescript-embed-lang": "^0.2.0"
  },
  "packageManager": "yarn@3.2.1"
