  "dependencies": {
    "@rescript/core": "0.5.0",
    "concurrently": "^8.2.2",
    "edgedb": "^1.3.7",
    "rescript": "11.0.0-rc.4",
    "rescript-edgedb": "^0.3.0",
    "rescript-embed-lang": "^0.2.0"
  },
  "packageManager": "yarn@3.2.1"
}