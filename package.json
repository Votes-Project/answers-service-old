{
  "name": "answers-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "bun test test/",
    "dev": "yarn build && concurrently -k -p \"[{name}]\" -n \"Rescript,App\" -c \"yellow.bold,cyan.bold\" \"yarn watch:edgedb\" \"yarn watch:res\"",
    "build": "yarn run build:edgedb && npm run build:res",
    "build:res": "rescript build",
    "watch:res": "rescript build -w",
    "build:edgedb": "rescript-edgedb generate --output ./src/__generated__ --src ./src",
    "watch:edgedb": "npm run build:edgedb -- --watch"
  },
